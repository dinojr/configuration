# ~/.bash_aliases
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'
alias s='cd ..'
alias d='ls'
alias p='cd -'
alias ls='ls --color'
alias g77='g77 -Wall'
alias pypar2='nice -n 15 pypar2'
alias file-roller='nice -n 15 file-roller'
alias par2='nice -n 15 par2'
alias unrar='nice -n 15 unrar'
alias advi='nice -n 10 /usr/bin/advi'
alias emacs='emacs-snapshot-gtk --fullheight'
alias beguiled='ssh beguiled.hd.free.fr'
